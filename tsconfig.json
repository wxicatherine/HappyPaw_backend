{
    "compilerOptions": {
      "module": "ESNext",
      "target": "ES6",
      "moduleResolution": "Node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    }
  }
  